{
  "name": "@ciscospark/react-ciscospark",
  "version": "0.1.0",
  "description": "The Cisco Spark for React library allows developers to easily incorporate Spark functionality into an application.",
  "scripts": {
    "build": "npm run build:packages && npm run build:bundle",
    "build:packages": "NODE_ENV=production babel-node --presets=stage-1,es2015 scripts/build-packages.js",
    "build:package": "NODE_ENV=production babel-node --presets=stage-1,es2015 scripts/build-package.js",
    "build:bundle": "NODE_ENV=production babel-node --presets=stage-1,es2015 scripts/build-bundle.js",
    "clean": "npm run clean:snapshots && npm run clean:dist",
    "clean:snapshots": "rimraf \"packages/**/__snapshots__/\"",
    "clean:dist": "rimraf \"packages/**/dist/\"",
    "test": "npm run static-analysis && npm run jest",
    "test:clean": "npm run clean && npm run test",
    "jest": "TZ=utc jest --config=jest.config.json",
    "static-analysis": "npm run eslint && npm run stylelint",
    "eslint": "eslint packages",
    "stylelint": "stylelint \"packages/node_modules/**/*.css\"",
    "start": "webpack-dev-server --config scripts/webpack/webpack.dev.babel.js --hot --inline"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ciscospark/react-ciscospark.git"
  },
  "contributors": [
    "Adam Weeks <adweeks@cisco.com>",
    "Bernie Zang <nzang@cisco.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ciscospark/react-ciscospark/issues"
  },
  "homepage": "https://github.com/ciscospark/react-ciscospark#readme",
  "main": "src/index.js",
  "devMain": "src/demo.js",
  "dependencies": {
    "@ciscospark/helper-html": "^0.7.34",
    "@ciscospark/plugin-avatar": "^0.7.31",
    "@ciscospark/plugin-conversation": "^0.7.31",
    "@ciscospark/plugin-flag": "^0.7.31",
    "@ciscospark/spark-core": "^0.7.31",
    "@ciscospark/storage-adapter-local-storage": "^0.7.31",
    "browser-saveas": "^1.0.1",
    "classnames": "^2.2.5",
    "immutable": "^3.8.1",
    "lodash": "^4.17.2",
    "marked": "^0.3.6",
    "moment": "^2.17.1",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-dropzone": "^3.9.0",
    "react-intl": "^2.2.2",
    "react-redux": "^5.0.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "reselect": "^2.5.4",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "@ciscospark/eslint-config": "^0.7.31",
    "@ciscospark/test-helper-file": "^0.7.31",
    "@ciscospark/test-helper-server": "^0.7.31",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-export-extensions": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-plugin-transform-strict-mode": "^6.18.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "babel-register": "^6.18.0",
    "child-process-promise": "^2.2.0",
    "css-loader": "^0.26.1",
    "denodeify": "^1.2.1",
    "dotenv": "^4.0.0",
    "eslint": "^3.14.0",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-mocha": "^4.8.0",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "html-webpack-plugin": "^2.24.1",
    "image-webpack-loader": "^3.1.0",
    "inline-environment-variables-webpack-plugin": "^1.1.0",
    "jest": "^18.0.0",
    "json-loader": "^0.5.4",
    "mkdirp": "^0.5.1",
    "postcss": "^5.2.6",
    "postcss-cssnext": "^2.9.0",
    "postcss-loader": "^1.2.1",
    "postcss-reporter": "^3.0.0",
    "react-test-renderer": "^15.4.1",
    "redux-logger": "^2.7.4",
    "redux-mock-store": "^1.2.1",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "stylelint": "^7.7.1",
    "stylelint-config-standard": "^15.0.1",
    "webpack": "^2.2.0-rc.3",
    "webpack-dev-server": "^2.2.0-rc.0"
  }
}
